    "build": "npx browserify _build/default/src/index.js -o _build/default/src/index.bs.js",
    "start": "yarn run build && npx http-server _build/default/src"
  },
  "devDependencies": {
    "browserify": "^17.0.0",
    "esy": "^0.6.10",
    "http-server": "^0.12.3"
  },
  "dependencies": {
    "bucklescript-tea": "^0.15.0"
  }
}
